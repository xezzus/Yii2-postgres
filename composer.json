{
    "name": "xezzus/yii2-postgres",
    "description": "The connect to data base Postgres.",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "extension",
        "postgres",
        "connect"
    ],
    "homepage": "https://github.com/xezzus/Yii2-postgres",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Buturlakin Andrey",
            "email": "xezzus@gmail.com",
            "homepage": "https://github.com/xezzus",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/xezzus/Yii2-postgres/issues",
        "source": "https://github.com/xezzus/Yii2-postgres"
    },
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "2.0.*"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
          "xezzus\\yii\\postgres\\" : ""
        }
    },
    "extra": {
        "branch-alias": {
          "dev-master":"0.x-dev"
        },
        "asset-installer-paths": {
            "bower-asset-library": "vendor/xezzus/yii2-postgres"
        }
    }
}
